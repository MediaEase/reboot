{
    "type": "project",
    "name": "mediaease/harmonyui",
    "description": "Harmony UI - frontend app for Harmony built with Symfony and TailwindCSS",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.12",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "2.*",
        "flagception/flagception-bundle": "^4.4",
        "lexik/jwt-authentication-bundle": "^2.20",
        "liip/imagine-bundle": "^2.12",
        "nelmio/api-doc-bundle": "4.26.*",
        "nelmio/cors-bundle": "^2.4",
        "nelmio/security-bundle": "^3.1",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.24",
        "pyrrah/gravatar-bundle": "^1.4",
        "sixlive/dotenv-editor": "^2.0",
        "symfony/asset": "7.0.*",
        "symfony/console": "7.0.*",
        "symfony/doctrine-messenger": "7.0.*",
        "symfony/dotenv": "7.0.*",
        "symfony/expression-language": "7.0.*",
        "symfony/filesystem": "7.0.*",
        "symfony/flex": "^2",
        "symfony/form": "7.0.*",
        "symfony/framework-bundle": "7.0.*",
        "symfony/http-client": "7.0.*",
        "symfony/intl": "7.0.*",
        "symfony/mailer": "7.0.*",
        "symfony/mercure-bundle": "^0.3.8",
        "symfony/mime": "7.0.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "7.0.*",
        "symfony/process": "7.0.*",
        "symfony/property-access": "7.0.*",
        "symfony/property-info": "7.0.*",
        "symfony/runtime": "7.0.*",
        "symfony/security-bundle": "7.0.*",
        "symfony/serializer": "7.0.*",
        "symfony/string": "7.0.*",
        "symfony/translation": "7.0.*",
        "symfony/twig-bundle": "7.0.*",
        "symfony/ux-dropzone": "^2.17",
        "symfony/ux-icons": "^2.17",
        "symfony/ux-live-component": "^2.17",
        "symfony/ux-turbo": "^2.17",
        "symfony/ux-twig-component": "^2.17",
        "symfony/validator": "7.0.*",
        "symfony/var-exporter": "7.0.*",
        "symfony/web-link": "7.0.*",
        "symfony/webpack-encore-bundle": "^2.1",
        "symfony/yaml": "7.0.*",
        "symfonycasts/reset-password-bundle": "^1.21",
        "symfonycasts/verify-email-bundle": "^1.15",
        "twig/cssinliner-extra": "^3.10",
        "twig/extra-bundle": "^3.8",
        "twig/inky-extra": "^3.10",
        "twig/intl-extra": "^3.8",
        "twig/markdown-extra": "^3.10",
        "twig/string-extra": "^3.8",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "analyze": [
            "@rector-bs",
            "@cs-bs",
            "@metrics",
            "@stan"
        ],
        "cs": "./vendor/bin/php-cs-fixer fix --verbose --allow-risky=yes --config=config/tools/.php-cs-fixer.dist.php",
        "cs-bs": "./vendor/bin/php-cs-fixer fix --verbose --allow-risky=yes --config=config/tools/.php-cs-fixer.dist.php --dry-run --format=txt > reports/phpcsfixer-baseline.txt || true",
        "fix": [
            "@rector",
            "@cs",
            "@insights"
        ],
        "insights": "./vendor/bin/phpinsights --format=console --no-interaction --fix",
        "insights-bs": "./vendor/bin/phpinsights --format=console --no-interaction -v > reports/phpinsights.txt || true",
        "metrics-src": "./vendor/bin/phpmetrics --level=10 --extensions=php8,php,inc src,tests --report-html=reports/metrics-src --report-violations=reports/metrics-src/violations.xml --exclude=DataFixtures",
        "metrics": [
            "@insights-bs"
        ],
        "rector": "./vendor/bin/rector process --config config/tools/rector.php --clear-cache --debug",
        "rector-bs": "./vendor/bin/rector process --config config/tools/rector.php --clear-cache --dry-run > reports/rector.txt || true",
        "stan": "./vendor/bin/phpstan analyze --configuration=config/tools/phpstan.neon --ansi -v || true"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "7.0.*",
            "docker": true
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.5",
        "flagception/flagception": "^1.9",
        "friendsofphp/php-cs-fixer": "^3.40",
        "phpmetrics/phpmetrics": "^0.0.1",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-symfony": "^1.3",
        "phpunit/phpunit": "11.1.*",
        "rector/rector": "1.*",
        "squizlabs/php_codesniffer": "^3.9",
        "symfony/browser-kit": "7.0.*",
        "symfony/css-selector": "7.0.*",
        "symfony/debug-bundle": "7.0.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "7.0.*",
        "symfony/web-profiler-bundle": "7.0.*"
    }
}
